    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "figlet": "^1.5.2",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-beta.0",
    "@playwright/test": "^1.30.0",
